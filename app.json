{
  "expo": {
    "name": "NBA",
    "slug": "nbaapp",
    "description":"NBA Details App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0054a4"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package":"nba.gimna.lk",
      "versionCode":1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#0054a4"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
